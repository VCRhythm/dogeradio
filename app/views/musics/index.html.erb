<div class="container">
	<div class="col-md-12">
		<h1>Music</h1>
		<ul>
			<% @musics.each do |music| %>
				<li><%= music.id %> - <%= link_to "Delete", music_delete_path(music), confirm: 'Are you sure you want to delete '+music.id+'?'%>
				</li>
			<% end %>
		</ul>
		<%= link_to 'New Music', new_music_path %>
	</div>
</div>
