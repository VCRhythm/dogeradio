<div class="container">
	<div class="col-md-12">
		<h1>Music</h1>
		<ul>
			<% @musics.each do |music| %>
				<li><%= music.key %> - <%= link_to "Delete", "musics/delete/?music="+music.key, confirm: 'Are you sure you want to delete '+music.key+'?'%>
				</li>
			<% end %>
		</ul>
		<%= link_to 'New Music', new_music_path %>
	</div>
</div>
