<% tracks.each do |track| %>
	<p>
		<%= link_to playlist_ranks_path(current_user.queue, music_id: track.id), method: :post, remote: true, class: "list-group-item" do %>
			<span class="badge" style="background-color:white"><span class="glyphicon glyphicon-plus" style="color:green"></span></span>
			<strong><%= track.name%></strong> by <%=track.user.username %>
	<% end %>
	</p>
<% end %>
