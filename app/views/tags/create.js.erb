$('#action-panel').slideUp('slow');
var player_track = $("#player-heading").data('music_id');
if (player_track == <%=@music.id%>) {
	$("#player-tags").html("<%= j render partial: 'show_tags', locals: {track: @music} %><button type='button' id='add-tag' class='btn btn-xs btn-default'><span class='glyphicon glyphicon-plus'></span><span class='glyphicon glyphicon-tag'></span></button>");
}
